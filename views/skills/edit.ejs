<%- include('../partials/header', { title: 'Developer Skills' }) %>

<form action="/skills/<%= skill.id %>?_method=PUT" method="POST">
  <input type="text" name="skill" value="<%= skill.skill %>"><br>
  <label>
    <input type="checkbox" name="done" <%= skill.accquired? 'checked' : '' %>> Accquired

  </label><br>
  <button type="submit">Update skill</button>
</form>

<%- include('../partials/footer') %>