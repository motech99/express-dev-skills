<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <title>Express Skills</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>Skills</h1>
    <ul>
        <% skills.forEach(function(s) { %>
          <li>
  
            <a href="/skill/<%= s.id %>"><%= s.skill %></a>
              - 
            <%= s.accquired ? 'accquired' : 'not accquired' %>
          </li>
        <% }); %>
      </ul>
  </body>
</html>