<%- include('../partials/header', { title: 'Developer Skills' }) %>

<h3>Developer skills as of <%= time %></h3>

<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <form
        action="/skills/<%= s.id %>?_method=DELETE"
        method="POST"
        class="delete-form"
      >
        <button type="submit">X</button>
      </form>
      <a href="/skills/<%= s.id %>"><%= s.skill %></a>
      - 
      <%= s.accquired ? 'accquired' : 'not accquired' %>
    </li>
  <% }); %>
</ul>
<a href="/skills/new">Add skill</a>

<%- include('../partials/footer') %>
